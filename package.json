{
  "name": "nuxt-wcferry",
  "version": "0.1.3",
  "description": "适用于 Nuxt 的微信机器人框架",
  "repository": "https://github.com/mrrhq/nuxt-wcferry",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && pnpm client:build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm build && bumpp",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.0",
    "consola": "^3.2.3",
    "croner": "^8.1.1",
    "hookable": "^5.5.3",
    "wechaty": "^1.20.2",
    "wechaty-puppet-wcferry": "^0.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.14",
    "@nuxt/eslint-config": "^0.5.2",
    "@nuxt/module-builder": "^0.8.3",
    "@nuxt/schema": "^3.13.0",
    "@nuxt/test-utils": "^3.14.1",
    "@types/node": "^22.5.0",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "nuxt": "^3.13.0",
    "typescript": "latest",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  },
  "packageManager": "pnpm@9.8.0+sha512.8e4c3550fb500e808dbc30bb0ce4dd1eb614e30b1c55245f211591ec2cdf9c611cabd34e1364b42f564bd54b3945ed0f49d61d1bbf2ec9bd74b866fcdc723276"
}
